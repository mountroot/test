package human;

public class Test {

	public static void main(String[] args) {

		Person taro = new Person();
		taro.name="山田太郎";
		taro.age=20;
		System.out.println(taro.name);
		System.out.println(taro.age);

		taro.talk();
		taro.walk();
		taro.run();

		Robot aibo = new Robot();
		aibo.name="アイボ";
		Robot asimo = new Robot();
		asimo.name="アシモ";
		Robot pepper = new Robot();
		pepper.name="ペッパー";
		Robot doraemon = new Robot();
		doraemon.name = "ドラエモン";
	}

}
